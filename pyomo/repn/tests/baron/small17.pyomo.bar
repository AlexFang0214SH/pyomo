OPTIONS {
Summary: 0;
}

POSITIVE_VARIABLES ONE_VAR_CONST__, x1, x2, x3;

LOWER_BOUNDS{
x1: 2;
x2: 0;
x3: 1;
}

EQUATIONS c_e_FIX_ONE_VAR_CONST__, c1, c2;

c_e_FIX_ONE_VAR_CONST__:  ONE_VAR_CONST__  == 1;
c1: x1 + x2 == 1;
c2: x3 + x2 == 1;

OBJ: minimize x1 + x3;

STARTING_POINT{
ONE_VAR_CONST__: 1;
x1: 1;
x2: 1;
x3: 1;
}

